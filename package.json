{
  "name": "naopoyo-web",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@8.7.6",
  "scripts": {
    "dev": "next dev",
    "build": "pnpm shiki && next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "prettier": "prettier --write '{src,test}/**/{*.ts,*.tsx}'",
    "format": "pnpm prettier && pnpm lint:fix",
    "codegen": "gql-gen --config codegen.ts",
    "shiki": "mkdir -p src/shiki && cp -r node_modules/shiki/{languages,themes} src/shiki/"
  },
  "dependencies": {
    "@graphql-typed-document-node/core": "3.2.0",
    "@urql/core": "4.1.4",
    "@urql/next": "1.1.0",
    "date-fns": "^2.30.0",
    "date-fns-tz": "^2.0.0",
    "graphql": "16.8.1",
    "mustache": "4.2.0",
    "next": "13.5.6",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "^4.11.0",
    "react-markdown": "9.0.0",
    "rehype-slug": "6.0.0",
    "remark-gfm": "4.0.0",
    "shiki": "0.14.5",
    "tocbot": "4.21.2",
    "unist-util-visit": "5.0.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "5.0.0",
    "@graphql-codegen/client-preset": "4.1.0",
    "@types/hast": "3.0.2",
    "@types/mustache": "4.2.4",
    "@types/node": "20.8.7",
    "@types/react": "18.2.31",
    "@types/react-dom": "18.2.14",
    "@types/unist": "3.0.1",
    "autoprefixer": "10.4.16",
    "dotenv": "16.3.1",
    "eslint": "8.52.0",
    "eslint-config-next": "13.5.6",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-import": "2.28.1",
    "eslint-plugin-tailwindcss": "3.13.0",
    "eslint-plugin-unused-imports": "3.0.0",
    "postcss": "8.4.31",
    "prettier": "3.0.3",
    "sass": "1.69.4",
    "tailwindcss": "3.3.3",
    "typescript": "5.2.2"
  }
}
