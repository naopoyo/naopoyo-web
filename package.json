{
  "name": "naopoyo-web",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "analyze": "ANALYZE=true next build",
    "build": "next build --turbopack",
    "check": "pnpm test:run && pnpm tsc --noEmit --incremental && pnpm lint",
    "dev": "next dev --turbopack -p 3001",
    "dev:debug": "NODE_OPTIONS='--inspect' next dev",
    "format": "pnpm prettier:write && pnpm lint:fix",
    "format:package": "NODE_OPTIONS='--no-warnings=ExperimentalWarning' pnpm dlx sort-package-json",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "local:setup": "sh scripts/setup_local.sh",
    "ncu": "pnpm dlx npm-check-updates",
    "ncu:minor": "pnpm ncu -u --target minor",
    "playwright:install": "pnpm dlx playwright install chromium --with-deps",
    "prettier:check": "prettier --check --ignore-unknown .",
    "prettier:write": "prettier --write --ignore-unknown .",
    "start": "next start",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:run": "vitest run"
  },
